<!DOCTYPE html>
<html>
<head>
<title>Online Shopping</title>
<!-- for-mobile-apps -->
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="keywords" content="Super Market, Online Shopping" />

<!-- //for-mobile-apps -->

<link href="~/CSS/css/bootstrap.css" rel="stylesheet" />

<link href="~/CSS/css/style.css" rel="stylesheet" />
<link rel="stylesheet" type="text/css" href="css/jquery-ui-1.css">

<!-- font-awesome icons -->
<link href="~/CSS/css/font-awesome.css" rel="stylesheet" />
<!-- //font-awesome icons -->
<!-- js -->
<script src="~/Scripts/js/jquery-1.11.1.min.js"></script>
<!-- //js -->
</head>
	
<body>
    
    <!-- header -->
    @Html.Partial("ThanhTran_Header")
    <!-- //header -->
    <!-- main navigation -->
    <div class="navigation-agileits">

        @Html.Partial("ThanhTran_Mainnavigation")

    </div>
    <!-- //main navigation -->
    <!-- filter -->
    @Html.Action("Loaddropdownlist", "Main")
    <!-- //filter -->
    <!--- main content --->
<div class="products">
    <div class="container">
        <div class="content">
            @RenderBody()
            @Html.Action("Loadproduct", "Main")
        </div>
    </div>
</div>

    <!--- main content --->
    <!-- footer -->
    @Html.Partial("ThanhTran_Footer")
    <!-- //footer -->
</body>
</html>